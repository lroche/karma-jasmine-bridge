{
  "name": "karma-jasmine-bridge",
  "version": "1.0.0",
  "description": "A bridge between Jasmine 1 to Jasmine 2 for Karma Env",
  "main": "lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "karma start --single-run",
    "testdev": "karma start --single-run=false --browsers Firefox,Chrome",
    "testdev-ie": "karma start --single-run=false --browsers Firefox,Chrome,IE"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lroche/karma-jasmine-bridge"
  },
  "keywords": [
    "Karma",
    "Jasmine",
    "Bridge",
    "runs",
    "waitsFor"
  ],
  "author": "Lionel Roche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lroche/karma-jasmine-bridge/issues"
  },
  "homepage": "https://github.com/lroche/karma-jasmine-bridge",
  "dependencies": {
    "proxy-polyfill": "^0.3.0"
  },
  "devDependencies": {
    "dojo": "^1.11.0",
    "eslint": "^5.13.0",
    "jasmine-core": "3.0.0",
    "karma": "^4.2.0",
    "karma-browserstack-launcher": "^1.5.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-dojo": "github:lroche/karma-dojo",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0"
  }
}
